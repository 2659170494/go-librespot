{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "$ref": "#/definitions/config",
  "definitions": {
    "config": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "log_level": {
          "type": "string",
          "enum": ["panic", "fatal", "error", "warn", "warning", "info", "debug", "trace"],
          "default": "info"
        },
        "device_name": {
          "type": "string",
          "default": "go-librespot"
        },
        "audio_device": {
          "type": "string",
          "default": ""
        },
        "server_port": {
          "type": "integer",
          "default": 0
        },
        "auth_method": {
          "type": "string",
          "enum": ["password", "spotify_token","zeroconf"],
          "default": "zeroconf"
        },
        "username": {
          "type": "string",
          "default": ""
        },
        "password": {
          "type": "string",
          "default": ""
        },
        "token": {
          "type": "string",
          "default": ""
        },
        "bitrate": {
          "type": "number",
          "default": 160
        }
      },
      "required": [
      ],
      "title": "config"
    }
  }
}
